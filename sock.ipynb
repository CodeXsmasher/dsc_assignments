      "source": [
        "from collections import Counter\n",
        "def socks(n, ar):\n",
        "  n=input()\n",
        "  ar = input().strip().split()\n",
        "  soc=0\n",
        "  for value in Counter(ar).values():\n",
        "    soc+=value//2\n",
        "    return soc\n",
        "socks(n, ar)\n",
        "\n"
      ],
